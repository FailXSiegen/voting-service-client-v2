{
  "name": "voting-service-client",
  "version": "2.0.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:e2e": "start-server-and-test preview :4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'",
    "test:load": "npx playwright test tests/load-test/load-test.spec.js",
    "test:load:simple": "npx playwright test tests/load-test/load-test.spec.js --project=chromium --grep \"Simple load test\"",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier src --write"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@ckeditor/ckeditor5-vue": "^7.3.0",
    "@popperjs/core": "^2.11.8",
    "@vue/apollo-composable": "^4.0.0",
    "@vue/apollo-option": "^4.0.0",
    "@vue/apollo-util": "^4.0.0-beta.2",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vuepic/vue-datepicker": "^7.2.2",
    "@vueuse/head": "^2.0.0",
    "@zoomus/websdk": "^2.18",
    "apollo-link-context": "^1.0.20",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.2",
    "ckeditor5": "^45.0.0",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.14.2",
    "jquery": "^3.7.1",
    "pinia": "^2.1.7",
    "vue": "^3.3.9",
    "vue-i18n": "^9.8.0",
    "vue-router": "^4.2.5",
    "vue3-easy-data-table": "^1.5.47",
    "vue3-toastify": "^0.1.14",
    "vuedraggable": "^4.1.0",
    "vuejs-confirm-dialog": "^0.5.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@rollup/plugin-inject": "^5.0.5",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/test-utils": "^2.4.3",
    "csv-parser": "^3.2.0",
    "cypress": "^13.6.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.19.2",
    "happy-dom": "^20.0.7",
    "jsdom": "^23.0.1",
    "prettier": "3.1.0",
    "sass": "^1.69.5",
    "start-server-and-test": "^2.0.3",
    "vite": "^5.4.11",
    "vite-plugin-static-copy": "^0.17.1",
    "vitest": "^3.2.4"
  },
  "engines": {
    "npm": ">=9.0.0",
    "node": ">=18.0.0"
  }
}
